{
  "name": "janashakthi-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for Janashakthi Financial Dashboard",
  "overrides": {
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "node test-api.js",
    "setup": "node seed-data.js",
    "setup:training": "npm install && node seed-data.js && echo 'Training environment ready! Run: npm run dev'",
    "migrate": "node scripts/migrate-to-database.js",
    "setup:db": "npm install && node scripts/migrate-to-database.js && npm run dev",
    "clean": "rm -rf uploads/* templates/*",
    "reset": "npm run clean && node seed-data.js",
    "health": "curl -s http://localhost:3001/api/health"
